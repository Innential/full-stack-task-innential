{
  "name": "frontend-react",
  "version": "0.1.0",
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-link-error": "^1.1.11",
    "apollo-link-http": "^1.5.15",
    "apollo-link-response-resolver": "^0.2.0",
    "autoprefixer": "^9.6.1",
    "graphql": "^14.5.3",
    "graphql-tag": "^2.10.1",
    "hash.js": "^1.1.3",
    "history": "4.7.2",
    "jwt-decode": "2.2.0",
    "postcss-cli": "^6.1.3",
    "prop-types": "15.6.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hook-form": "^3.22.3",
    "react-router-dom": "5.0.0",
    "react-scripts": "2.1.8",
    "react-toast-notifications": "^2.2.4",
    "styled-jsx": "2.2.5"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "npm-run-all build:css build:react",
    "build:css": "postcss src/styles/tailwind.css -o src/index.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/index.css -w",
    "start:react": "react-app-rewired start",
    "build:react": "react-app-rewired build",
    "dev": "PORT=8081 npm-run-all --parallel watch:css start:react",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "postinstall": "ln -sfn $(dirname $(pwd))/settings './src/settings'"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-root-import": "^6.0.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "customize-cra": "0.2.12",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.11.1",
    "prettier-eslint": "^8.8.1",
    "react-app-rewired": "2.1.0",
    "tailwindcss": "^1.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
