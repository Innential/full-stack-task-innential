{
  "name": "apollo-backend",
  "version": "0.1.0",
  "description": "",
  "main": "./src/server.js",
  "scripts": {
    "start": "bash start.sh",
    "start-docker": "bash start-docker.sh",
    "launch": "nodemon ./src/server.js --exec babel-node -e js",
    "add-component-part": "cd ./src/scripts && bash create-graphql-component.sh",
    "remove-component-part": "cd ./src/scripts && bash delete-graphql-component.sh",
    "build": "babel -d ./build ./src -s",
    "serve-staging": "NODE_ENV=staging node ./server.js",
    "serve-production": "NODE_ENV=production node ./server.js",
    "serve-testing": "yarn start",
    "test": "yarn serve-testing && wait %1 & jest",
    "test-travis": "jest"
  },
  "author": "",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-async-generator-functions": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-bulk-import": "ecerroni/babel-plugin-bulk-import#patch-1",
    "babel-plugin-root-import": "6.1.0",
    "eslint": "4.18.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-html": "4.0.2",
    "eslint-plugin-import": "2.9.0",
    "graphql-tester-options": "0.0.6",
    "jest": "23.6.0",
    "jwt-decode": "2.2.0",
    "nodemon": "1.18.7"
  },
  "dependencies": {
    "@babel/runtime": "7.2.0",
    "@okgrow/graphql-scalars": "^0.4.2",
    "apollo-server-express": "2.1.0",
    "apollo-server-module-operation-store": "1.3.2",
    "bcrypt": "2.0.0",
    "cookie-parser": "1.4.3",
    "cookie-session": "2.0.0-beta.3",
    "cors": "2.8.4",
    "eslint-plugin-graphql": "^2.1.1",
    "express": "4.16.2",
    "graphql": "^0.13.0",
    "graphql-tools": "4.0.3",
    "jsonwebtoken": "8.1.1",
